{
  "name": "@ianwalter/vuex-reset",
  "version": "1.0.2",
  "description": "A Vuex plugin that makes restoring initial state to the store simple",
  "main": "dist/vuex-reset.js",
  "umd:main": "dist/vuex-reset.umd.js",
  "module": "dist/vuex-reset.m.js",
  "scripts": {
    "build": "microbundle build index.js --compress false",
    "build:site": "webpack --env.singleRun",
    "watch": "microbundle watch index.js --compress false",
    "start": "webpack-dev-server",
    "lint": "eslint --ext js index.js tests",
    "test": "npm run build && jest"
  },
  "keywords": [
    "vue",
    "vuex",
    "restore",
    "reset",
    "initial",
    "state"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "SEE LICENSE IN LICENSE",
  "dependencies": {
    "@ianwalter/clone": "^1.0.2",
    "deepmerge": "^2.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/preset-env": "^7.1.6",
    "@fullhuman/purgecss-loader": "^1.0.0",
    "@ianwalter/dracula": "^2.0.0",
    "@ianwalter/prism-loader": "^1.2.0",
    "babel-loader": "^8.0.4",
    "bootstrap": "^4.1.3",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.7.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "highlight.js": "^9.13.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "microbundle": "^0.7.0",
    "mini-css-extract-plugin": "^0.4.5",
    "np": "^3.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "style-loader": "^0.23.1",
    "vue": "^2.5.17",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.5.17",
    "vuex": "^3.0.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/vuex-reset.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/vuex-reset/issues"
  },
  "homepage": "https://github.com/ianwalter/vuex-reset#readme",
  "eslintConfig": {
    "root": true,
    "env": {
      "jest/globals": true
    },
    "extends": [
      "standard"
    ],
    "plugins": [
      "jest"
    ]
  }
}
