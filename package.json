{
  "name": "@ianwalter/vuex-reset",
  "version": "1.0.1",
  "description": "A Vuex plugin that makes restoring initial state to the store simple",
  "main": "dist/vuex-reset.js",
  "umd:main": "dist/vuex-reset.umd.js",
  "module": "dist/vuex-reset.m.js",
  "scripts": {
    "build": "microbundle build index.js --compress false",
    "watch": "microbundle watch index.js --compress false",
    "lint": "eslint --ext js index.js tests",
    "test": "npm run build && jest"
  },
  "keywords": [
    "vue",
    "vuex",
    "restore",
    "reset",
    "initial",
    "state"
  ],
  "author": "Ian Walter <public@iankwalter.com> (https://iankwalter.com)",
  "license": "ISC",
  "dependencies": {
    "@ianwalter/clone": "^1.0.2",
    "deepmerge": "^2.2.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^23.6.0",
    "microbundle": "^0.6.0",
    "np": "^3.0.4",
    "vue": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianwalter/vuex-reset.git"
  },
  "bugs": {
    "url": "https://github.com/ianwalter/vuex-reset/issues"
  },
  "homepage": "https://github.com/ianwalter/vuex-reset#readme",
  "eslintConfig": {
    "root": true,
    "env": {
      "jest/globals": true
    },
    "extends": [
      "standard"
    ],
    "plugins": [
      "jest"
    ]
  }
}
